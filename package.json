{
  "name": "root",
  "version": "0.0.1",
  "description": "library use framework project deboxsoft.",
  "private": true,
  "maintainers": [
    {
      "name": "Nurdiansyah",
      "email": "nur.diansyah.ckt@gmail.com"
    }
  ],
  "scripts": {
    "clean": "yarn workspaces run clean",
    "format": "",
    "build": "tsc -b",
    "typecheck:svelte": "lerna exec svelte-check -- --workspace ./",
    "typecheck:prettier": "lerna exec prettier -- --write --plugin-search-dir ./ \"**/*.svelte\"",
    "typecheck:eslint": "lerna exec eslint -- --fix .",
    "typecheck:tsc": "lerna exec tsc -- --noEmit --composite false",
    "typecheck": "run-p typecheck:prettier typecheck:eslint typecheck:tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
