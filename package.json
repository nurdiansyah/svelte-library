    "@storybook/svelte": "5.3.19",
    "@sucrase/webpack-loader": "^2.0.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
