{
  "bracketSpacing": true,
  "semi": true,
  "trailingComma": "none",
  "plugins": ['prettier-plugin-svelte'],
  "overrides": [
    {
      "files": [".js", ".jsx"],
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.{ts,tsx}",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": ["yaml", "yml"],
      "options": {
        "parser": "yaml"
      }
    }
  ]
}
